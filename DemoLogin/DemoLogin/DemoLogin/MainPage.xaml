<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             xmlns:local="clr-namespace:DemoLogin"
             x:Class="DemoLogin.MainPage">
    <ContentPage.Resources>
        <ResourceDictionary>
            <Style x:Key="styStackLayout" TargetType="StackLayout">
                <Setter Property="" Value=""/>
            </Style>
        </ResourceDictionary>
    </ContentPage.Resources>
    <StackLayout>
        <StackLayout>
            <Label Text="Acount"/>
            <Entry Placeholder="Acount"/>
        </StackLayout>
        <StackLayout>
            <Label Text="Password"/>
            <Entry Placeholder="Password"/>
        </StackLayout>
        <StackLayout>
            <Button Text="Login"/>
            
        </StackLayout>
        
        
    </StackLayout>

</ContentPage>
